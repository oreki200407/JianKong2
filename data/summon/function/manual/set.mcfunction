function summon:operation/summon/cooldown
data modify storage jk2:data root.summon.manual.name set from entity @s ArmorItems[2].components."minecraft:custom_name"
data modify storage jk2:data root.summon.manual.lore set from entity @s ArmorItems[2].components."minecraft:lore"
data modify storage jk2:data root.summon.manual.profile set from entity @s ArmorItems[2].components."minecraft:profile"